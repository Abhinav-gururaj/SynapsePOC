									"name": "CHARGE 1 STATUTE"
								}
							},
							{
								"source": {
									"path": "$['charge_1_description']"
								},
								"sink": {
									"name": "CHARGE 1 DESCRIPTION"
								}
							},
							{
								"source": {
									"path": "$['charge_1_type']"
								},
								"sink": {
									"name": "CHARGE 1 TYPE"
								}
