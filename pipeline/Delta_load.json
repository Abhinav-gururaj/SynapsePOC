				"userProperties": [],
				"typeProperties": {
					"dataflow": {
						"referenceName": "Stage_to_DWH",
						"type": "DataFlowReference",
						"parameters": {
							"date": {
								"value": "'@{variables('current_date')}'",
								"type": "Expression"
							}
						},
						"datasetParameters": {
							"SrcSynapse": {
								"DBName": "mydedicatedsqlpool",
								"schema": "Assign",
								"table": "Transaction"
							},
							"TgtSynapse": {
								"DBName": "mydedicatedsqlpool",
								"schema": "Assign",
								"table": "Transaction"
							}
						}
					},
					"staging": {
						"linkedService": {
							"referenceName": "synapsepoc1321-WorkspaceDefaultStorage",
							"type": "LinkedServiceReference"
						},
						"folderPath": "myfilesystem/Staging"
					},
					"compute": {
						"coreCount": 8,
						"computeType": "General"
					},
					"traceLevel": "Fine"
				}
			}
		],
		"variables": {
			"current_date": {
				"type": "String"
			}
		},
		"folder": {
			"name": "Assignment"
		},
