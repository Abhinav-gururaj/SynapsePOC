					{
						"activity": "Create_log",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"userProperties": [],
				"typeProperties": {
					"variableName": "current_date",
					"value": {
						"value": "@formatDateTime(utcnow(),'yyyy/MM/dd')",
						"type": "Expression"
					}
				}
			},
			{
				"name": "Stage_to_DWH",
				"type": "ExecuteDataFlow",
				"dependsOn": [
					{
						"activity": "Update_Staging_date",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
